{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
  "parameters": {
    "envPrefix": {
      "type": "string",
      "defaultValue": "gdu",
      "metadata": {
        "description": "Common prefix for all resource naming"
      }
    },
    "envLocation": {
      "type": "string",
      "defaultValue": "West Europe",
      "allowedValues": [
        "North Europe",
        "West Europe"
      ],
      "metadata": {
        "description": "Location for all resources"
      }
    },
    "artifactsLocation": {
      "type": "string",
      "defaultValue": "https://raw.githubusercontent.com/shosho-san/ResourceTemplates/master/RSG_Infrastructure/RSG_Infrastructure",
      "metadata": {
        "description": "Storage Account/GitHub base uri for required files"
      }
    }
  },
  "variables": {
    
  },
  "resources": [
    {
      "name": "Network",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "dependsOn": [ ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('artifactsLocation'), '/Templates/NestedDeploy/virtualnetwork.json')]",
          "contentVersion": "1.0.0.0"
        }
      }
    },
    {
      "name": "Storage",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "dependsOn": [ ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('artifactsLocation'), '/Templates/NestedDeploy/storage.json')]",
          "contentVersion": "1.0.0.0"
        }
      }
    }
  ],
    "outputs": {
    }
}
